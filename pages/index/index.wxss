.index-swiper{
    width: 750rpx;
    height: 340rpx;
}

.index-swiperItem image{
    width: 100%;
}

.index-cart{
    display: flex;
}

.index-cart>.index-cartList{
    flex: 1;
    justify-content: center;
    align-items: center;
    padding: 20rpx;
}

.index-cartList image{
    width: 100%;
}

.floor-item{
    width: 750rpx;
    padding: 10rpx;
    background-color: #f3eded;
}


.floor-item image{
    width: 100%;
    padding: 5rpx 0;
}

.floor-list{
    width: 33.33%;
    float: left;
}

/* 后四个超链接的样式 */
/* :nth-last-child伪类元素 */
.floor-content navigator:nth-last-child(-n+4){
    /* 33.33%/h=232/386 */
    height: calc(378*33.33vw /233 /2);
    border-left: 5px solid #fff;
}

.floor-content navigator:nth-child(2),
.floor-content navigator:nth-child(3)
{
    border-bottom: 5px solid #fff;
}

.floor-list image{
    width: 100%;
    height: 100%;
}